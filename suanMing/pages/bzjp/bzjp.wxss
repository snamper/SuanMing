}

.personal-secret {
  color: #ff8b58;
  text-decoration: underline;
  font-size: 16px;
}

checkbox .wx-checkbox-input {
  border-radius: 5px;
  width: 32rpx;
  height: 32rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  color: #444;
}

