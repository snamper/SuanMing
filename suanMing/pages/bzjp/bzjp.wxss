}

.immediately-open {
  background-color: #fff2db;
  border-radius: 10px;
  border: 1px solid #f3d59f;
  margin: 10px 15px 15px;
  color: #535353;
  height: 26px;
  line-height: 26px;
  text-align: center;
  font-size: 14px;
}

.personal-information {
  margin: 10px 10px 0;
  background-color: #fff9db;
  border: 1px solid #f3d59f;
}

.personal-information-item {
  height: 40px;
  line-height: 40px;
  padding: 0 5px;
  border-bottom: 1px solid #f3d59f;
  display: flex;
  justify-content: space-between;
}

.item {
  height: 40px;
  line-height: 40px;
  padding: 0 5px;
  border-bottom: 0;
  display: flex;
  justify-content: space-between;
}

.label {
  color: #a61222;
  margin-right: 10px;
  font-size: 14px;
}

.content {
  width: 76%;
}

.content input {
  width: 100%;
  height: 40px;
  line-height: 40px;
  padding: 5px 0;
  box-sizing: border-box;
  font-size: 16px;
}

radio .wx-radio-input {
  border-radius: 100%;
  width: 30rpx;
  height: 30rpx;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  border-radius: 100%; /* 圆角 */
  width: 38rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  height: 38rpx; /* 选中后对勾大小，不要超过背景的尺寸 */
  line-height: 38rpx;
  text-align: center;
  font-size: 30rpx; /* 对勾大小 30rpx */
  color: #fff; /* 对勾颜色 白色 */
  background-color: #a61222;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

.radio-man {
  margin-right: 40rpx;
}

.content text {
  color: #333;
  font-size: 16px;
}

.picker {
  width: 100%;
  box-sizing: border-box;
  height: 40px;
  line-height: 40px;
  padding: 0 4px;
  font-weight: 400;
  font-size: 16px;
}

.down-arrow {
  margin: 22rpx 8rpx 0 0;
  width: 16rpx;
  height: 16rpx;
  border-bottom: 1rpx solid #a61222;
  border-right: 1rpx solid #a61222;
  transform: rotate(45deg);
  float: right;
}

.born-time-view {
  line-height: 40px;
  height: 40px;
}

.agree-protocol {
