.title {
  text-align: center;
  font-size: 40rpx;
  color: #f94990;
  margin: 0 44px;
  font-weight: 400;
  line-height: 88rpx;
  height: 88rpx;
}

.banner-img {
  width: 100%;
  height: 242rpx;
}

.subheading {
  height: 40px;
  line-height: 40px;
  margin-top: 10px;
  border-bottom: 1px solid #999;
  text-align: center;
  font-size: 18px;
  background-color: #ececec;
  color: #f94990;
  font-weight: 700;
}

.type {
  margin: 30rpx 20rpx;
  display: flex;
}

.type-img {
  width: 160rpx;
  height: 160rpx;
}

.type-percent {
  color: #f94990;
  font-size: 28rpx;
  line-height: 36rpx;
  text-align: center;
}

.type-evaluate {
  color: #f00;
  font-size: 28rpx;
  line-height: 36rpx;
  text-align: center;
}

.type-evaluate text {
  color: #292929;
  font-size: 28rpx;
  line-height: 36rpx;
}

.type-title {
  height: 60rpx;
  line-height: 60rpx;
  font-weight: 700;
  color: #282828;
  font-size: 36rpx;
}

.type-text {
  color: #474747;
  font-size: 32rpx;
}

.type-left {
  margin-right: 20rpx;
}

.immediately-measure {
  margin: 10rpx 20rpx 30rpx 20rpx;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #f94990;
  font-size: 36rpx;
  color: #fff;
  border-radius: 10rpx;
}

.footer {
  margin: 40rpx 0 0 0;
  background-color: #999;
  overflow: hidden;
  width: 100%;
  padding: 40rpx 0;
  color: #fff;
  text-align: center;
  font-size: 28rpx;
  line-height: 1.4;
}

.we {
  display: flex;
  justify-content: center;
  margin-top: 2rpx;
}

.about-us {
  margin-right: 24rpx;
}
