    <view class='my-measure'>
      我的测算
    </view>
  </view>
  <view>
    <image src='http://ffsm.smdqw.net/statics/ffsm/xmpeidui/2/images/banner.jpg' class='banner-img'></image>
  </view>
  <view class='form-view'>
    <image src='http://ffsm.smdqw.net/statics/ffsm/xmpeidui/2/images/banner_2.jpg' class='img2'></image>
    <view class='form-container'>
      <form class='name-form'>
        <view class='input-box'>
          <view class='male'>
            <view class='sex-text'>男主</view>
            <view class='name'>
              <text class='name-text'>姓氏:</text>
              <input placeholder="输入姓" class='name-input'></input>
            </view>
            <view class='name'>
              <text class='name-text'>名字:</text>
              <input placeholder="输入名" class='name-input'></input>
            </view>
          </view>
          <view class='female'>
            <view class='sex-text'>女主</view>
            <view class='name'>
              <text class='name-text'>姓氏:</text>
              <input placeholder="输入姓" class='name-input'></input>
            </view>
            <view class='name'>
              <text class='name-text'>名字:</text>
              <input placeholder="输入名" class='name-input'></input>
            </view>
          </view>
          <view style='clear:both'></view>
        </view>
        <view class='agree-protocol'>
          <checkbox checked='true' color='#424242'></checkbox>
          <text>同意</text>
          <text class='personal-secret'>个人隐私协议</text>
        </view>
        <view class='begin-matching'>
          <button class='matching-btn'>开始配对</button>
          <view class='think-precise'>
            已有
            <text class='number'>1594691</text>人觉得很准
          </view>
        </view>
      </form>
    </view>
  </view>
  <view class='empty-view'></view>
  <view class='harvest'>
    <view class='harvest-text'>测算后您将获得</view>
    <view>
      <image src='http://ffsm.smdqw.net/statics/img/site1.png' class='img3'></image>
