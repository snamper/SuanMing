      <image src='http://ffsm.smdqw.net/statics/img/site3.png' class='img3'></image>
      <image src='http://ffsm.smdqw.net/statics/img/site4.png' class='img4'></image>
      <image src='http://ffsm.smdqw.net/statics/img/site5.png' class='img4'></image>
      <image src='http://ffsm.smdqw.net/statics/img/site6.png' class='img4'></image>
    </view>
