page {
  background-color: #f94990;
}

.top {
  height: 88rpx;
  line-height: 88rpx;
  background-color: #fff;
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.top-icon {
  width: 48rpx;
  height: 48rpx;
  margin-left: 16rpx;
}

.xmpd {
  font-size: 40rpx;
  color: #f94990;
  height: 60rpx;
  line-height: 60rpx;
}

.my-measure {
  height: 60rpx;
  line-height: 60rpx;
  border: 2rpx solid #f94990;
  border-radius: 10rpx;
  padding: 0 10rpx;
  color: #f94990;
  font-size: 28rpx;
  margin-right: 16rpx;
}

.banner-img {
  width: 100%;
  height: 173rpx;
  display: block;
}

.form-view {
  position: relative;
}

.img2 {
  width: 100%;
  height: 528rpx;
  display: block;
}

.form-container {
  position: absolute;
  width: 100%;
  height: 90%;
  top: 0;
  left: 0;
}

.name-form {
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  position: relative;
  top: 50%;
}

.input-box {
  margin: 0 10px;
}

.male {
  float: left;
  padding: 5px 10px 0 20px;
  width: 50%;
  background: url(http://ffsm.smdqw.net/statics/ffsm/xmpeidui/2/images/name_bg.png) top center no-repeat;
  background-size: 90% auto;
  box-sizing: border-box;
}

.female {
  float: right;
  padding: 5px 20px 0 10px;
  width: 50%;
  background: url(http://ffsm.smdqw.net/statics/ffsm/xmpeidui/2/images/name_bg.png) top center no-repeat;
  background-size: 90% auto;
  box-sizing: border-box;
}

.sex-text {
  line-height: 28px;
  color: #fff;
  font-size: 20px;
  text-align: center;
}

.name {
  position: relative;
  padding-left: 40px;
  height: 28px;
  color: #fff;
  margin: 8px 0 0;
}

.name-text {
  position: absolute;
  height: 28px;
  width: 40px;
  left: 0;
  top: 0;
  line-height: 28px;
  font-size: 16px;
}

.name-input {
  height: 28px;
  border: 1px solid #ccc;
  width: 88%;
  box-sizing: border-box;
  border-radius: 3px;
  padding: 0 4px;
  background-color: white;
  color: #333;
  font-size: 16px;
}

.agree-protocol {
  color: #fff;
  padding: 6px 10px 0;
  font-size: 14px;
  position: relative;
  text-align: center;
  line-height: 1.4;
}

.personal-secret {
  color: #ff8b58;
  text-decoration: underline;
  font-size: 14px;
  line-height: 1.4;
  font-weight: bolder;
  margin-left: 4rpx;
}

checkbox .wx-checkbox-input {
  border-radius: 3px;
  width: 28rpx;
  height: 28rpx;
}

.begin-matching {
  position: relative;
}

.matching-btn {
  width: 124px;
  height: 36px;
  background: url(http://ffsm.smdqw.net/statics/ffsm/xmpeidui/2/images/btn_bg.png) no-repeat;
  background-size: 100% 120%;
  margin: 0 auto;
  line-height: 36px;
  font-size: 18px;
}

.think-precise {
  color: #fff;
  padding: 2px 0 0;
  text-align: center;
  font-size: 14px;
  line-height: 1.4;
}

.number {
  color: #ffe713;
  padding: 0 4px;
  font-weight: bold;
  font-size: 14px;
  line-height: 1.4;
}

.empty-view {
  padding: 5px 10px 10px;
  color: #fffbda;
  font-size: 16px;
  line-height: 24px;
}

.harvest {
  margin: 5px 10px 0;
  background-color: #fff;
  border-radius: 3px;
  padding: 0 0 10px;
}

.harvest-text {
  border-bottom: 1px solid #d7d7d7;
  font-size: 20px;
  padding: 6px 0;
  text-align: center;
  font-weight: 400;
  line-height: 1.4;
  color: #333;
}

.img3 {
  width: 100%;
  height: 762rpx;
}

.img4 {
  width: 100%;
  height: 710rpx;
}

.feedback {
  margin: 5px 10px 0;
  background-color: #fff;
  border-radius: 3px;
  padding: 0 0 10px;
}

.feedback-title {
  border-bottom: 1px solid #d7d7d7;
  font-size: 20px;
  padding: 6px 0;
  text-align: center;
  line-height: 1.4;
  color: #333;
}

.feedback-box {
  height: 400px;
  overflow: hidden;
}

.feedback-item {
  margin: 0 10px;
  padding: 10px;
  border-bottom: 1px solid #d7d7d7;
  height: 120px;
  font-size: 14px;
}

.item-title {
  font-size: 16px;
  color: #ff6c00;
  margin-bottom: 5px;
  line-height: 1.4;
}

.love-state {
  display: inline-block;
  color: #fff;
  border: 1px solid #d7d7d7;
  border-radius: 5px;
  padding: 0 10px;
  background-color: #d10b3b;
  margin-left: 5px;
  font-size: 14px;
  line-height: 1.4;
}

.feedback-detail {
  font-size: 14px;
  line-height: 1.4;
  color: #333;
}

.me-too {
  width: 124px;
  height: 36px;
  background: url(http://ffsm.smdqw.net/statics/ffsm/xmpeidui/2/images/btn_bg.png) no-repeat;
  background-size: 100% 120%;
  margin: 15px auto;
  line-height: 36px;
  text-align: center;
  font-size: 18px;
  color: #333;
}

.popular-measure-container {
  border: 1px solid #d3d3d3;
  border-radius: 5px;
  margin: 10px;
  background-color: #fff;
}

.popular-measure {
  border-bottom: 1px solid #d3d3d3;
  height: 24px;
  line-height: 24px;
  padding: 10px;
  color: #000;
  font-weight: 800;
  text-align: center;
  font-size: 16px;
}

.popular-measure-view {
  overflow: hidden;
  padding: 5px 0;
}

.popular-measure-item {
  float: left;
  width: 33%;
  padding: 5px 5px 0;
  box-sizing: border-box;
}

.measure-img {
  width: 100%;
}

.measure-text {
  line-height: 24px;
  height: 26px;
  font-size: 14px;
  color: #222;
  text-align: center;
  overflow: hidden;
}

.footer {
  width: 100%;
  text-align: center;
  color: #fff;
  padding: 20px 0 50px 0;
  font-size: 12px;
  line-height: 1.4;
}

.we {
  display: flex;
  justify-content: center;
  margin-top: 2rpx;
}

.about-us {
  margin-right: 24rpx;
}
