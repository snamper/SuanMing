page {
  background-color: #e8e8e8;
}

.top {
  height: 88rpx;
  line-height: 88rpx;
  background-color: #fff;
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.top-icon {
  width: 48rpx;
  height: 48rpx;
  margin-left: 16rpx;
}

.xmxp {
  font-size: 40rpx;
  color: #f94990;
  height: 60rpx;
  line-height: 60rpx;
}

.my-measure {
  height: 60rpx;
  line-height: 60rpx;
  border: 2rpx solid #f94990;
  border-radius: 10rpx;
  padding: 0 10rpx;
  color: #f94990;
  font-size: 28rpx;
  margin-right: 16rpx;
}

.banner-img {
  width: 100%;
  height: 192rpx;
  display: block;
}

.tab-bar {
  display: flex;
  border-bottom: 1px solid #d6d6d6;
  background-color: #fff;
}

.name-explain-bar {
  width: 50%;
  position: relative;
}

.my-measure-bar {
  width: 50%;
}

.name-explain {
  color: #dd863c;
  border-bottom: 2px solid #dd863c;
  margin-bottom: -1px;
  width: 80%;
  height: 40px;
  line-height: 40px;
  margin: 0 auto;
  text-align: center;
  font-size: 14px;
}

.bar-divider-line {
  height: 30px;
  width: 1px;
  background-color: #d6d6d6;
  position: absolute;
  top: 5px;
  right: 0;
}

.bar-my-measure {
  width: 80%;
  height: 40px;
  line-height: 40px;
  margin: 0 auto;
  text-align: center;
  color: #333;
  font-size: 14px;
}

.form-view {
  margin: 10px 10px 0;
}

.form-container {
  border: 1px solid #d3d3d3;
  border-radius: 5px;
  -webkit-border-radius: 5px;
  background-color: #fff;
}

.form-row {
  height: 40px;
  line-height: 40px;
  border-bottom: 1px solid #d3d3d3;
  padding: 0 5px;
  display: flex;
  justify-content: space-between;
}

.form-label {
  color: #f94990;
  margin-right: 10px;
  line-height: 40px;
  font-size: 14px;
}

.form-content {
  flex-grow: 2;
}

.input-area {
  width: 100%;
  height: 40px;
  line-height: 40px;
  padding: 5px 0;
  box-sizing: border-box;
  font-size: 16px;
}

radio .wx-radio-input {
  border-radius: 100%;
  width: 30rpx;
  height: 30rpx;
}

.sex-radio-group {
  width: 100%;
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  color: #333;
}

.man-radio {
  margin-right: 40rpx;
}

.picker {
  width: 100%;
  box-sizing: border-box;
  height: 40px;
  line-height: 40px;
  padding: 0 4px;
  font-weight: 400;
  font-size: 16px;
}

.right-arrow {
  margin: 26rpx 8rpx 0 0;
  width: 16rpx;
  height: 16rpx;
  border-bottom: 1rpx solid #f94990;
  border-right: 1rpx solid #f94990;
  transform: rotate(-45deg);
  float: right;
}

.last-form-row {
  height: 40px;
  line-height: 40px;
  padding: 0 5px;
  display: flex;
  justify-content: space-between;
}

.born-time-view {
  line-height: 40px;
  height: 40px;
}

.agree-protocol {
  padding: 8px 10px;
  text-align: center;
  font-size: 16px;
  line-height: 1.4;
  color: #333;
}

.personal-secret {
  color: #ff8b58;
  text-decoration: underline;
  font-size: 16px;
  line-height: 1.4;
}

checkbox .wx-checkbox-input {
  border-radius: 4rpx;
  width: 26rpx;
  height: 26rpx;
}

.immediately-explain-name {
  padding: 10px 0;
  margin: 0 10px;
}

.explain-name-btn {
  height: 34px;
  line-height: 34px;
  text-align: center;
  font-size: 18px;
  width: 100%;
  background-color: #f94990;
  color: #fff;
  border-radius: 5px;
}

.product-introduction-container {
  border: 1px solid #e2ccb0;
  margin: 10px 10px 0;
  border-radius: 5px;
  background: #fff;
}

.product-introduction {
  height: 30px;
  line-height: 30px;
  padding: 0 10px;
  border-bottom: 1px solid #e2ccb0;
  font-size: 14px;
  color: #333;
}

.influence-container {
  padding: 10px;
  border-bottom: 1px solid #d7d7d7;
}

.explain-text {
  font-size: 18px;
  color: #d52800;
  text-align: center;
  line-height: 1.4;
}

.influence-explain-text {
  color: #6e5233;
  text-align: center;
  font-size: 14px;
  margin: 5px 0;
  line-height: 1.4;
}

.img2 {
  width: 100%;
  height: 690rpx;
}

.unlock-container {
  padding: 10px;
}

.unlock-explain-text {
  color: #6e5233;
  font-size: 14px;
  margin: 5px 0 10px 0;
  line-height: 1.4;
}

.img3 {
  width: 100%;
  height: 457rpx;
}

.popular-measure-container {
  border: 1px solid #d3d3d3;
  border-radius: 5px;
  margin: 10px;
  background-color: #fff;
}

.popular-measure {
  border-bottom: 1px solid #d3d3d3;
  height: 24px;
  line-height: 24px;
  padding: 10px;
  color: #000;
  font-weight: 800;
  text-align: center;
  font-size: 16px;
}

.popular-measure-view {
  overflow: hidden;
  padding: 5px 0;
}

.popular-measure-item {
  float: left;
  width: 33%;
  padding: 5px 5px 0;
  box-sizing: border-box;
}

.measure-img {
  width: 100%;
}

.measure-text {
  line-height: 24px;
  height: 26px;
  font-size: 14px;
  color: #222;
  text-align: center;
  overflow: hidden;
}

.footer {
  width: 100%;
  text-align: center;
  color: #6c6c6c;
  padding: 20px 0 50px 0;
  font-size: 12px;
  line-height: 1.4;
}

.we {
  display: flex;
  justify-content: center;
  margin-top: 2rpx;
}

.about-us {
  margin-right: 24rpx;
}
